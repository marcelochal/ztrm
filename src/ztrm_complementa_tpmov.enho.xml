   <ENHANCEMENTS>
    <ENH_HOOK_IMPL>
     <PROGRAMNAME>RFIDCFM_BR_TAXFUNDS_ME</PROGRAMNAME>
     <ENHMODE>D</ENHMODE>
     <FULL_NAME>\PR:RFIDCFM_BR_TAXFUNDS_ME\FO:FI_TRAN_POSTING_EXECUTE\SE:BEGIN\EI</FULL_NAME>
     <SOURCE>
      <item>*  Completa o lançamento com o tipo de movimento</item>
      <item>DATA: lt_MOV_TRM   TYPE STANDARD TABLE OF ZFI_MOV_TRM,</item>
      <item>ls_mov_trm   TYPE zfi_mov_trm,</item>
      <item>lt_ftpost    TYPE STANDARD TABLE OF FTPOST,</item>
      <item>ls_FTPOST    TYPE FTPOST.</item>
      <item/>
      <item/>
      <item>LOOP AT ftpost INTO ls_ftpost.</item>
      <item/>
      <item>APPEND ls_ftpost TO lt_ftpost.</item>
      <item>if ls_ftpost-FNAM  = &apos;BSEG-HKONT&apos;.</item>
      <item/>
      <item>SELECT SINGLE * INTO ls_mov_trm</item>
      <item>FROM zfi_mov_trm</item>
      <item>WHERE DIS_FLOWTYPE  = gs_taxcust-post_spec</item>
      <item>AND HKONT         = ls_ftpost-fval.</item>
      <item/>
      <item>IF NOT sy-subrc IS INITIAL.</item>
      <item/>
      <item>SELECT SINGLE * INTO ls_mov_trm</item>
      <item>FROM zfi_mov_trm</item>
      <item>WHERE DIS_FLOWTYPE  = gs_taxcust-post_spec.</item>
      <item/>
      <item>ENDIF.</item>
      <item/>
      <item>CHECK sy-subrc IS INITIAL.</item>
      <item/>
      <item>ls_ftpost-fnam       = &apos;COBL-RMVCT&apos;.</item>
      <item>ls_ftpost-fval       = ls_mov_trm-TRTYP.</item>
      <item>APPEND ls_ftpost TO lt_ftpost.</item>
      <item>ENDIf.</item>
      <item/>
      <item>ENDLOOP.</item>
      <item/>
      <item>ftpost[] = lt_ftpost.</item>
      <item/>
     </SOURCE>
    </ENH_HOOK_IMPL>
